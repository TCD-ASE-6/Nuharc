{
  "name": "nuharc",
  "version": "1.0.0",
  "description": "A Disaster Management Application for Dublin built using the MERN Stack",
  "main": "server.js",
  "scripts": {
    "start": "SET NODE_ENV=production & node server.js",
    "server": "SET NODE_ENV=production & nodemon server.js",
